{
    "lambdaFunction": {
      "lambdaArn": "arn:aws:lambda:us-east-1:{AccountId}:function:demo-greengrass-function:5",
      "componentName": "com.example.LambdaFunctionComponent",
      "componentVersion": "1.0.0",
      "componentPlatforms": [
        {
          "name": "Linux arm64",
          "attributes": {
            "os": "linux", 
            "architecture": "arm64"
          }
        },
        {  
          "name": "Linux aarch64",
          "attributes": {
            "os": "linux",
            "architecture": "aarch64"  
          }
        }
      ],
      "componentDependencies": {},
      "componentLambdaParameters": {
        "eventSources": [
          {
            "topic": "demo-greengrass/request",
            "type": "IOT_CORE"
          }
        ],
        "environmentVariables": {
          "AWS_REGION": "us-east-1",
          "RESPONSE_TOPIC": "demo-greengrass/response"
        },
        "inputPayloadEncodingType": "json",
        "pinned": false,
        "timeoutInSeconds": 3,
        "statusTimeoutInSeconds": 60,
        "maxIdleTimeInSeconds": 60,
        "maxInstancesCount": 100,
        "maxQueueSize": 1000,
        "linuxProcessParams": {
          "containerParams": {
            "memorySizeInKB": 65536
          }
        }
      }
    }
  }